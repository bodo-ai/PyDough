{
    "name": "PyDough",
    "build": {
      "dockerfile": "Dockerfile",
      "context": ".."
    },
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": [
                // Python
                "ms-python.python",
                "ms-python.vscode-pylance",
                // Jupyter
                "ms-toolsai.jupyter"
            ]
        },
        "codespaces": {
            "openFiles": [
                "demos/README.md",
                "demos/notebooks/Introduction.ipynb"
            ]
        },
        "postCreateCommand": "python -m pip install . && ./demos/setup_tpch.sh ./demos/tpch.db"
    }
}
